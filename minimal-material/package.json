{
  "name": "minimal-material",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "compile": "next build && next export",
    "spec": "npm run spec/firefox -- $@",
    "spec/firefox": "npm run cy --run=firefox -- $@",
    "spec/chrome": "npm run cy --run=chrome -- $@",
    "prespec": "cy_png",
    "postspec": "find ./cypress/integration/__image_snapshots__/* -type d | xargs -d'\\n' rm -rf && exit 0 nyc",
    "cy": "cy $@",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mdc.build": "sass origin_use.scss origin.css --no-source-map --load-path=node_modules"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@kirmas/template-cypress": "file:configs/cypress",
    "@kirmas/template-dev": "file:configs/devDependencies",
    "@kirmas/template-next": "file:configs/next",
    "material-components-web": "^7.0.0",
    "material-design-lite": "^1.3.0"
  },
  "dependencies": {
    "update": "^0.7.4"
  }
}
